+			p.store(new FileWriter("Resources/ValorParametro.properties",true), "Nueva Propiedad: "+Nombre );
+		} catch (IOException e) {
+			// TODO Auto-generated catch block
+			e.printStackTrace();
+		}
+	}//Cierre del m�todo
+	
+	
